[Unit]
Description=MQTT Kamera Controller

[Service]
Type=simple
ExecStart=/usr/bin/python3 /home/pi/MQTT/MQTT_KameraController.py
Restart=on-failure
Environment=PYTHONPATH=/home/pi/non-git-local-includes
WorkingDirectory=/home/pi/MQTT/
StandardOutput=null
StandardError=null

[Install]
WantedBy=multi-user.target
