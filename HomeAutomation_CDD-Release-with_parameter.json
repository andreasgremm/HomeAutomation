      "skipTasksWithUnchangedApplications": false,
      "ownerParties": [
        "andreas.gremm@broadcom.com"
      ],
      "description": "<p>Phase zum Test der Github Integration.</p>",
      "name": "QA Docker Images",
      "kind": "Phase"
    },
    {
      "release": "${ReleaseName}/${ReleaseVersion}",
      "approvalGate": "ON_FAILURE",
