      "description": "<p>Build the Docker Container for HueController.</p>",
      "name": "Build HueModule Docker Container",
      "kind": "Task",
      "endpoint": "Jenkins EP",
      "plugin": "Jenkins/1.3.0",
      "parameters": {
        "build parameters": "{\"parameter\": [{\"name\":\"JENKINSPATH\", \"value\":\"DockerEnv/Hue\"}, {\"name\":\"DOCKERTAG\", \"value\":\"huemodule:prod\"}]}",
        "project name": "BuildDockerContainer",
        "taskExpirationTime": "43200"
      },
      "type": "Run Build"
    },
    {
      "phase": "${ReleaseName}/1/QA Docker Images",
      "applicationVersions": [
        "Local|HueController/${HueControllerAppVersion}"
      ],
      "isDisabled": false,
      "previousTasks": [
        "Build HueModule Docker Container"
      ],
      "description": "<p>Build the Docker Container for HueController.</p>",
      "name": "Build HueController Docker Container",
      "kind": "Task",
      "endpoint": "Jenkins EP",
      "plugin": "Jenkins/1.3.0",
      "parameters": {
        "build parameters": "{\"parameter\": [{\"name\":\"JENKINSPATH\", \"value\":\"DockerEnv/MQTT_HueController\"}, {\"name\":\"DOCKERTAG\", \"value\":\"huecontroller:prod\"}]}",
        "project name": "BuildDockerContainer",
        "taskExpirationTime": "43200"
      },
      "type": "Run Build"
    },
    {
      "phase": "${ReleaseName}/1/QA Docker Images",
      "applicationVersions": [
        "Local|HueController/${HueControllerAppVersion}"
      ],
      "isDisabled": false,
      "previousTasks": [
        "Build HueController Docker Container"
      ],
      "tags": [
        "Mett"
      ],
      "name": "Test Task",
      "kind": "Task",
      "plugin": "ADAPTIVE TESTING/1.1.1",
      "parameters": {
        "changeId": "%HueController.last_successful_change%",
        "runOnlySubset": "false"
      },
      "type": "Run Adaptive Testing"
    },
    {
      "phase": "${ReleaseName}/1/QA Docker Images",
      "applicationVersions": [
        "Local|RadioController/${RadioControllerAppVersion}",
        "Local|HueController/${HueControllerAppVersion}"
      ],
      "isDisabled": false,
      "previousTasks": [
        "Test Task"
      ],
      "description": "<p>Sende eine Slack Message als Information\t</p>",
      "name": "Inform Andreas Finish",
      "kind": "Task",
      "endpoint": "Slack EP",
      "plugin": "Slack/1.1",
      "parameters": {
        "channel": "#general",
        "message": "Build finished: %HueController.last_successful_change%",
        "username": "CDDirector"
      },
      "type": "Post message"
    },
    {
      "phase": "${ReleaseName}/1/Failure Phase",
      "isDisabled": false,
      "name": "Failure Task",
      "kind": "Task"
