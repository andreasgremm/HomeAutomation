      "name": "Inform Andreas",
      "kind": "Task",
      "endpoint": "Slack EP",
      "plugin": "Slack/1.1",
      "parameters": {
        "channel": "#general",
        "message": "%Phase_Start_Message%: %EnvironmentToken/Environment_Memory%",
        "username": "CDDirector"
      },
      "type": "Post message"
    },
    {
      "phase": "${ReleaseName}/1/QA Docker Images",
