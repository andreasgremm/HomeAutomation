    {
      "release": "${ReleaseName}/${ReleaseVersion}",
      "scope": "PHASE_SCOPE",
      "value": "BuildDockerContainer",
      "name": "Jenkins_Project_RepositoryList",
      "kind": "ReleaseToken"
    },
    {
      "release": "${ReleaseName}/${ReleaseVersion}",
      "scope": "PHASE_SCOPE",
      "value": "QA Started",
